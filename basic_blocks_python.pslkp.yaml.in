pluginName: basic_blocks_python
pluginType: coreBlockLibrary
blockType: BasicPython
version: @PROJECT_VERSION@
dependsOn:
  - name: BlockTypesBasicPython
    version: @PROJECT_VERSION@
blockLibraries:
  - name: BasicBlocksPython
    blockTypes:
      - name: BasicPython
        configurationValues:
          - name: PythonModule
            defaultValue: ""
            type: string
          - name: PythonClass
            defaultValue: ""
            type: string
